doctype html
html
	head
		title= title
		link(rel='stylesheet', href='/stylesheets/style.css')
		link(rel='stylesheet', href='https://storage.googleapis.com/code.getmdl.io/1.0.0/material.teal-orange.min.css')
		link(rel='stylesheet', href='https://fonts.googleapis.com/icon?family=Material+Icons')
	body
		div.mdl-layout.mdl-js-layout.mdl-layout--fixed-header
			header.mdl-layout__header
				div.mdl-layout__header-row
					span.mdl-layout-title Jon Davenport
					div.mdl-layout-spacer
					nav.mdl-navigation.mdl-layout--large-screen-only
						a.mdl-navigation__link(href="/Contact") Contact
						a.mdl-navigation__link(href="/Resume") Resume
		
			div.mdl-layout__content
				block content

			footer.mdl-mini-footer
				div.mdl-mini-footer--left-section
					div.mdl-logo
						Jon Davenport
